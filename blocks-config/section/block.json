{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "version": "0.1.0",
    "name": "premium/section",
    "title": "Section",
    "description": "Engage your Gutenberg blocks using Premium Section Block.",
    "keywords": ["section"],
    "textdomain": "premium-blocks-for-gutenberg",
    "category": "premium-blocks",
    "attributes": {
        "stretchSection": {
            "type": "boolean",
            "default": false
        },
        "innerWidthType": {
            "type": "string",
            "default": "boxed",
            "isStyle": true
        },
        "isUpdated": {
            "type": "boolean",
            "default": false
        },
        "horAlign": {
            "type": "object",
            "default": {
                "Desktop": "center",
                "Tablet": "center",
                "Mobile": "center"
            },
            "isStyle": true
        },
        "height": {
            "type": "string",
            "default": "min",
            "isStyle": true
        },
        "innerWidth": {
            "type": "number",
            "isStyle": true
        },
        "minHeight": {
            "type": "number",
            "isStyle": true
        },
        "minHeightUnit": {
            "type": "string",
            "default": "px",
            "isStyle": true
        },
        "vPos": {
            "type": "string",
            "default": "top",
            "isStyle": true
        },
        "blockId": {
            "type": "string"
        },
        "padding": {
            "type": "object",
            "default": {
                "Desktop": {
                    "top": "",
                    "right": "",
                    "bottom": "",
                    "left": ""
                },
                "Tablet": {
                    "top": "",
                    "right": "",
                    "bottom": "",
                    "left": ""
                },
                "Mobile": {
                    "top": "",
                    "right": "",
                    "bottom": "",
                    "left": ""
                },
                "unit": {
                    "Desktop": "px",
                    "Tablet": "px",
                    "Mobile": "px"
                },
                "link": true
            },
            "isStyle": true
        },
        "margin": {
            "type": "object",
            "default": {
                "Desktop": {
                    "top": "",
                    "right": "",
                    "bottom": "",
                    "left": ""
                },
                "Tablet": {
                    "top": "",
                    "right": "",
                    "bottom": "",
                    "left": ""
                },
                "Mobile": {
                    "top": "",
                    "right": "",
                    "bottom": "",
                    "left": ""
                },
                "unit": {
                    "Desktop": "px",
                    "Tablet": "px",
                    "Mobile": "px"
                },
                "link": true
            },
            "isStyle": true
        },
        "boxShadow": {
            "type": "object",
            "default": {
                "color": "transparent",
                "blur": "10",
                "horizontal": "0",
                "vertical": "0",
                "position": " "
            },
            "isStyle": true
        },
        "border": {
            "type": "object",
            "default": {
                "borderType": "none",
                "borderColor": "",
                "borderWidth": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "link": true
                },
                "borderRadius": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "link": true
                }
            },
            "isStyle": true
        },
        "background": {
            "type": "object",
            "default": {
                "backgroundType": "",
                "backgroundColor": "",
                "backgroundImageID": "",
                "backgroundImageURL": "",
                "backgroundPosition": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": ""
                },
                "backgroundRepeat": {
                    "Desktop": "no-repeat",
                    "Tablet": "",
                    "Mobile": ""
                },
                "backgroundSize": {
                    "Desktop": "cover",
                    "Tablet": "",
                    "Mobile": ""
                },
                "fixed": false,
                "gradientLocationOne": "0",
                "gradientColorTwo": "",
                "gradientLocationTwo": "100",
                "gradientAngle": "180",
                "gradientPosition": "center center",
                "gradientType": "linear"
            },
            "isStyle": true
        },
        "classes": {
            "type": "string",
            "default": ""
        }
    },
    "editorScript": "pbg-blocks-js",
    "editorStyle": "premium-blocks-editor-css"
}
